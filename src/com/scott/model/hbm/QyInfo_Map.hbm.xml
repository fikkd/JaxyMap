<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.scott.model" auto-import="true">
	<class name="QyInfo_Map" table="qyinfo_map">
		<id name="id">
			<column name="id" not-null="true" length="32" />
			<generator class="assigned">
			</generator>
		</id>
		<property name="qymc" lazy="false">
			<column name="qymc" length="300" />
		</property>
		<property name="m_lng" lazy="false">
			<column name="m_lng" length="15" />
		</property>
		<property name="m_lat" lazy="false">
			<column name="m_lat" length="15" />
		</property>
		<property name="m_count" lazy="false">
			<column name="m_count" length="10" />
		</property>
	</class>
</hibernate-mapping>