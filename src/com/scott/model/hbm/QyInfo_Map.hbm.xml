<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.scott.model" auto-import="true">
	<class name="QyInfo_Map" table="qyinfo_map">
		<id name="id">
			<column name="id" not-null="true" length="32" />
			<generator class="assigned">
			</generator>
		</id>
		<property name="c_name" lazy="false">
			<column name="c_name" length="500" />
		</property>
		<property name="c_social_credit_code" lazy="false">
			<column name="c_social_credit_code" length="30" />
		</property>
<!-- 		
		<property name="estart_time" type="date" lazy="false">
			<column name="estart_time" />
		</property> -->
		<property name="legal_representative" lazy="false">
			<column name="legal_representative" length="512" />
		</property>
		<property name="res_address" lazy="false">
			<column name="res_address" length="200" />
		</property>
<!-- 		
		<property name="reg_capital" lazy="false">
			<column name="reg_capital" length="12" />
		</property> -->
		<property name="m_lng" lazy="false">
			<column name="m_lng" length="15" />
		</property>
		<property name="m_lat" lazy="false">
			<column name="m_lat" length="15" />
		</property>
	</class>
</hibernate-mapping>